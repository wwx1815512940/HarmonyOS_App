<div class="list-container">
    <div class="list-item" for="{{ v in list}}" tid="v.id"  >
        <image class="cover" src="{{ v.imgPathUrl }}"
               object-fit="cover"></image>
        <text class="name">{{v.title}}</text>
        <div class="author">
            <image src="{{ v.photoUrl }}" ></image>
            <text>{{ v.createOperatorName }}</text>
            <text>{{v.thumbUpNum}}</text>
        </div>
    </div>
</div>